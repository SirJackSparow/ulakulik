<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/splashFragment">

    <include-dynamic
        android:id="@+id/loginGraph"
        app:graphResName="login_nav"
        app:moduleName="login" />

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.newskeletoncompose.view.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_loginGraph"
            app:destination="@id/loginGraph" />
    </fragment>

</navigation>
